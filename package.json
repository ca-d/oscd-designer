{
  "name": "@omicronenergy/oscd-editor-sld",
  "version": "0.0.1",
  "description": "OpenSCD Menu Plugin for loading IEC 61850 SCL files into OpenSCD.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OMICRONEnergyOSS/oscd-editor-sld.git"
  },
  "bugs": {
    "url": "https://github.com/OMICRONEnergyOSS/oscd-editor-sld/issues"
  },
  "homepage": "https://openscd.org",
  "keywords": [
    "OpenSCD",
    "iec81650"
  ],
  "author": "OMICRON Electronics GmbH",
  "license": "Apache-2.0",
  "exports": {
    ".": "./dist/oscd-editor-sld.js",
    "./oscd-editor-sld.js": "./dist/oscd-editor-sld.js"
  },
  "files": [
    "/dist/",
    "!/dist/*.spec.*",
    "!/dist/*.test.*"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "clean": "rimraf --glob 'dist/!(__snapshots__)'",
    "analyze": "cem analyze --litelement --exclude demo/* --exclude dist/* --exclude *.spec.ts --exclude *.test.ts --exclude coverage/*",
    "demo:copy": "cp -r ./demo ./dist/",
    "demo:watch": "chokidar './demo/**/*' --initial -c 'npm run demo:copy'",
    "markdown:build": "marked -i about.md -o dist/about.html",
    "markdown:watch": "chokidar 'about.md' --initial -c 'npm run markdown:build'",
    "start": "npm run build && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"chokidar './demo/**/*' -c 'npm run copy-demo'\" \"wds --node-resolve --hmr \"",
    "start:bundle": "npm run bundle && concurrently -k -r \"rollup -c rollup.config.js --watch\" \"wds --watch \"",
    "build": "npm run clean && npm run demo:copy && npm run markdown:build && npm run analyze && tsc ",
    "build:watch": "npm run clean && concurrently -k -r \"npm run demo:watch\" \"npm run markdown:watch\" \"tsc --watch --preserveWatchOutput\"",
    "bundle": "npm run clean && npm run demo:copy && npm run markdown:build && npm run analyze && rollup -c rollup.config.js",
    "test": "npm run build && wtr --coverage ",
    "test:watch": "concurrently -k -r \"npm run build:watch\" \"sleep 5 && wtr --watch\"",
    "deploy": "npm run bundle && gh-pages -d dist -b deploy",
    "prepare": "husky"
  },
  "dependencies": {
    "@material/mwc-button": "^0.27.0",
    "@material/mwc-dialog": "^0.27.0",
    "@material/mwc-fab": "^0.27.0",
    "@material/mwc-icon": "^0.27.0",
    "@material/mwc-icon-button": "^0.27.0",
    "@material/mwc-icon-button-toggle": "^0.27.0",
    "@material/mwc-list": "^0.27.0",
    "@material/mwc-menu": "^0.27.0",
    "@material/mwc-snackbar": "^0.27.0",
    "@material/mwc-textfield": "^0.27.0",
    "@omicronenergy/oscd-api": "^0.1.3",
    "@omicronenergy/oscd-background-editv1": "^0.0.6",
    "@omicronenergy/oscd-background-wizard-events": "^0.0.1",
    "@omicronenergy/oscd-menu-open": "^0.0.6",
    "@omicronenergy/oscd-menu-save": "^0.0.2",
    "@omicronenergy/oscd-shell": "^0.0.9",
    "@openscd/oscd-scl": "^0.0.2",
    "lit": "^3.3.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@custom-elements-manifest/analyzer": "^0.10.10",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@omicronenergy/oscd-editor": "^1.5.0",
    "@open-wc/dev-server-hmr": "^0.2.0",
    "@open-wc/eslint-config": "^13.0.0",
    "@open-wc/testing": "4.0.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@ungap/structured-clone": "^1.3.0",
    "@web/dev-server": "^0.4.6",
    "@web/rollup-plugin-html": "^2.3.0",
    "@web/rollup-plugin-import-meta-assets": "^2.3.0",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-lit-a11y": "^5.1.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-wc": "^3.0.2",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "marked": "^16.4.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "rollup-plugin-copy": "^3.5.0",
    "sinon": "^21.0.0",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@web/test-runner-playwright": {
      "playwright": "1.54.1"
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "customElements": "custom-elements.json"
}
